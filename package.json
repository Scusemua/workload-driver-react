{
  "name": "distributed-notebook-dashboard",
  "version": "0.0.2",
  "description": "An open source dashboard for a Distributed Jupyter cluster.",
  "repository": "https://github.com/Scusemua/workload-driver-react",
  "homepage": "https://github.com/Scusemua/workload-driver-react",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build-server": "go build ./cmd/server/main.go -o server",
    "build-grpc": "protoc --go_out=. --go_opt=paths=source_relative --go-grpc_out=. --go-grpc_opt=paths=source_relative server/api/proto/gateway.proto",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "start:dev-spoofed": "webpack serve --color --progress --config webpack.dev.js --env spoof",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./web/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./web/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.4",
    "@storybook/addon-actions": "^8.0.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^8.0.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react-router-dom": "^5.3.3",
    "@types/victory": "^33.1.5",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "imagemin": "^8.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^6.22.3",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.5",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@jupyterlab/services": "^7.1.6",
    "@melloware/react-logviewer": "^5.0.5",
    "@patternfly/react-charts": "^7.2.2",
    "@patternfly/react-code-editor": "^5.2.3",
    "@patternfly/react-core": "^5.2.3",
    "@patternfly/react-icons": "^5.2.1",
    "@patternfly/react-styles": "^5.2.1",
    "@patternfly/react-table": "^5.2.4",
    "@storybook/builder-webpack5": "^8.0.6",
    "ansi_up": "^6.0.2",
    "console-feed": "^3.6.0",
    "font-awesome": "^4.7.0",
    "jest-fetch-mock": "^3.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-use-websocket": "^4.8.1",
    "sirv-cli": "^2.0.2",
    "stable-hash": "^0.0.4",
    "swr": "^2.2.5",
    "uuid": "^9.0.1"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "tls": false,
    "child_process": false
  }
}
